    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "mglaman/composer-drupal-lenient": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"],

        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            },
            "excludes": [
                ".htaccess"
            ],
            "initial": {
                "assets/scaffold/files/htaccess": ".htaccess"
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/spectrum": ["npm-asset/spectrum-colorpicker"],
            "libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable_patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "Custom blocks break layout builder module - Quick Edit could not associate the rendered entity field markup": "https://www.drupal.org/files/issues/2023-02-13/3072231-85.patch",
                "Reorder Layout Builder sections": "https://www.drupal.org/files/issues/2024-06-13/3080606-121.patch",
                "Expose Layout Builder data to REST and JSON:API": "https://www.drupal.org/files/issues/2024-12-20/2942975-273_0.patch",
                "PHP message: Error: Call to a member function getEntityTypeId() on null (Layout Builder)": "./patches/core/broken_block.patch",
                "Image align center not working" : "./patches/core/align_center.patch",
                "Add SMS Protocol": "./patches/core/add-sms-protocol.patch",
                "Warning: mb_strlen() expects parameter 1 to be string, FormValidator.php.":  "https://www.drupal.org/files/issues/2022-09-16/mb-strlen-expects-parameter-form-validator-3310057-D9-7.patch",
                "Another datetime fix": "./patches/core/datetime-fix.patch",
                "ConditionManager::evaluate() should not negate results itself": "https://www.drupal.org/files/issues/2022-10-18/2535896-40.patch",
                "Add support for negating node type(content type) condition for block visibility": "https://www.drupal.org/files/issues/2024-01-08/drupal-3264843-21.patch",
                "Call to a member function setSyncing() on null when installing a module": "https://www.drupal.org/files/issues/2020-08-18/protect_against_invalid_ids_in_import_delete-3051453-10.patch",
                "Allow tokens for url of the Link field": "https://www.drupal.org/files/issues/2021-07-02/2903336-40-allow-tokens-in-link-url-include-external.patch",
                "Fix for creating links with no route": "./patches/core/fix_programatic_link_creation.patch",
                "Deprecated function: dirname(): Passing null to parameter #1 ($path) of type string is deprecated": "https://www.drupal.org/files/issues/2024-06-26/3268634_core_dirname_38.patch",
                "EditorFileReference should compute a px <img height> if a % <img width> is specified, even though % <img width> is not allowed in HTML5": "https://www.drupal.org/files/issues/2024-01-16/3336446_17.patch",
                "Fix link widget token detection": "./patches/core/link_token_fix.patch",
                "big_pipe.js' checkMutation() does not check if node exists before using it": "https://www.drupal.org/files/issues/2024-01-21/drupal-big_pipe_does_not_check_before_using_node_object-3416141.patch",
                "Update url validator to handle null values": "./patches/core/url_validator_patch.patch",
                "Make sure link item always returns a valid URL object when link is a token" : "./patches/core/link_token_fix2.patch",
                "Remove the redirect from aggregated assets": "./patches/core/remove_asset_redirect.patch",
                "Error when saving a denormalized entity with text fields with \"processed\" property": "https://git.drupalcode.org/project/drupal/-/merge_requests/1114.diff"
            },
            "drupal/layout_library": {
                "Add hide the content preview option to layout editing" : "https://www.drupal.org/files/issues/2019-11-07/3081497_add-hide-content-preview-02.patch"
            },

            "drupal/formatter_field": {
                "Drupal 9 readiness": "https://www.drupal.org/files/issues/2020-06-06/formatter_field.1.x-dev.rector.patch"
            },
            "drupal/we_megamenu": {
                "No Custom Blocks Available for selection": "https://www.drupal.org/files/issues/2021-04-15/2925829-13.patch"
            },
            "drupal/config_split": {
                "Wildcard exclusion support": "https://www.drupal.org/files/issues/2019-04-03/3036622-config-split-exclude-1.patch"
            },
            "drupal/cer": {
                "Non valid entities getting added to fields": "https://www.drupal.org/files/issues/2021-11-04/3239049_2.patch"
            },
            "drupal/linkchecker": {
                "Handle empty redirects": "./patches/linkchecker/handle_empty_redirects.patch"
            },
            "drupal/mailgun": {
                "Incomplete domain list": "https://www.drupal.org/files/issues/2022-04-21/3276471.patch",
                "Error: Interface \"Drush\\Commands\\sql\\SanitizePluginInterface\" not found in modules/contrib/mailgun/src/Commands/MailgunSanitizeCommands.php on line 16": "https://www.drupal.org/files/issues/2025-08-10/3540647-mailgun-interface-not-found.patch"
            },
            "drupal/protected_pages": {
                "Gives error if page does not have an alias": "https://www.drupal.org/files/issues/2022-04-28/3277694.patch"
            },
            "drupal/advagg": {
                "Fix error in CkEditor": "https://www.drupal.org/files/issues/2022-09-19/3310388-2.patch"
            },
            "drupal/colorbox_media_video" : {
                "Add responsive video option to field formatter": "https://www.drupal.org/files/issues/2023-02-24/3300344-colorbox_media_video--responsive-video-option-5.patch"
            },
            "drupal/views_autocomplete_filters": {
                "Undefined index: autocomplete_field breaking search view ajax": "https://www.drupal.org/files/issues/2022-03-22/views_autocomplete_filter-2999501-9-stable-dont_break_ajax_with_php_notice.patch"
            },
            "drupal/swiftmailer": {
                "Conflicts with drupal/core-recommended 10.1.x-dev requires egulias/email-validator ~4.0.1": "https://www.drupal.org/files/issues/2023-07-19/3359916.patch"
            },
            "drupal/bulk_update_fields": {
                "Add alter hooks to alter values before being saved to entities": "./patches/bulk_update_fields/add_hooks_updated.patch"
            },
            "drupal/cshs": {
                "Fix weird operand type error": "./patches/cshs/operand_fix.patch"
            },
            "drupal/draggableviews": {
                "Contextual Link throws RouteNotFoundException: Route \"view.this\" does not exist": "https://www.drupal.org/files/issues/2024-06-10/3316975-32-MR40.patch",
                "Row weights not displaying on sort view.": "https://www.drupal.org/files/issues/2023-08-25/3252365-check-remove-select-all-class.patch"
            },
            "drupal/media_thumbnails_svg": {
                "Account for imagemagick v7": "./patches/media_thumbnails_svg/magick-update.patch"
            },
            "drupal/token": {
                "Token tree performance improvement": "./patches/token/performance.patch",
                "Add more perfornance enhancements": "./patches/token/more-performant.patch"
            },
            "drupal/redirect": {
                "Setting 'Enforce clean and canonical URLs.' breaks CSS aggregation on multilingual Drupal 10.1.x with browser caching enabled": "https://www.drupal.org/files/issues/2024-09-28/3373123-60_redirect_dont-normalize-assets.patch",
                "Redirects from aliased paths aren't triggered": "https://www.drupal.org/files/issues/2024-08-11/redirect--2024-08-11--2879648--mr-109.patch"
            },
            "drupal/webform": {
                "Fix incompatibility with devel ^5.2": "https://www.drupal.org/files/issues/2024-08-01/fix_incompatibility_with_devel_5.2.patch"
            },
            "drupal/linkit": {
                "Show URL alias after autocomplete selection instead of internal route (e.g., /node/123)": "https://www.drupal.org/files/issues/2024-10-14/2877535-77-7.x.patch"
            },
            "drupal/rabbit_hole": {
                "rh-node.js error: $action.next(...).textContent is not a function": "https://www.drupal.org/files/issues/2024-07-30/3463826-rh-node-js-textContent-issue-3.patch"
            },
            "drupal/section_library": {
                "Add alter hooks after cloning": "./patches/section_library/alter_hooks.patch"
            },
            "drupal/lb_copy_section": {
                "Add alter hooks": "./patches/lb_copy_section/add_alters.patch"
            },
            "drupal/show_title": {
                "Drupal 10 readiness": "./patches/show_title/d10-compatibility2.patch"
            },
            "drupal/metatag": {
                "Token recursion fix": "https://www.drupal.org/files/issues/2025-08-08/fix_recursion.patch"
            }
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/vardumper", "drupal/ckeditor_tableselection", "drupal/show_title", "drupal/ckeditor_entity_link"]
        }
    },
    "scripts": {
        "removeSubmodules": "./bin/gitSubmoduleRemove.sh",
        "cerCommit": "./bin/gitCommit.sh 'modules/contrib/cer' 'commit patches'",
        "linkitCommit": "if [ -d modules/contrib/linkit/.git ]; then rm -rf modules/contrib/linkit/.git; fi",
        "normalize": "php ./bin/modeNormalize.php",
        "post-drupal-scaffold-cmd": [
            "chmod 755 sites/default && patch -p1 < post-scaffold-patch.patch"
        ],
        "post-install-cmd": [
            "@removeSubmodules"
        ],
        "post-update-cmd": [
            "@removeSubmodules"
        ]
    },
    "require-dev": {
        "drupal/core-dev": "^10",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/phpstan": "^1.12"
    }
}
